<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Newance_Training::elements" title="Training" sortOrder="10">
                        <resource id="Newance_Training::post" title="Posts" sortOrder="10" />
                        <resource id="Newance_Training::category" title="Categories" sortOrder="20" />
                        <resource id="Newance_Training::brand" title="Brands" sortOrder="30" />
